{
  "expo": {
    "name": "Spazzolino",
    "slug": "Spazzolino",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/main_icon.jpg",
    "scheme": "spazzolino",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.spazzolino.app",
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/icons/main_icon.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/main_icon.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-notifications"
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}